{
	"data" : {
		"fore" : ["a.txt", "b.txt"],
		"back" : [""]
	},

	"alphabet" : {
		"separator" : "",
		"groups" : {
			".": {"elements":"ACDEFGHIKLMNPRQSTVWY"}
		}
	},

	"cache" : {
		"p-value" : "[fore]-[back]-hyper-up-approx"
	},

	"extension" : {
		"method" : "group",
		"filter" : {
			"pat-groups" : {"max":3},
			"pat-length" : {"max":6},
			"matches(@)" : {"min":20},
			"seqs(@)" : {"min": 10},
			"no-starting-group" : {}
		}
	},

	"output" : {
		"order" : "p-value",
		"sort" : "desc",
		"filter" : {
			"hyper-up-approx(fore, back)" : {"max":1e-3},
			"seqs(fore)" : {},
			"matches(fore)": {},
			"occs(fore)" : {},

			"ratio(fore, *, ~fore)" : {"min":2},
			"hyper-X-min(fore...)" : {"min":1e-3},
			"hyper-find-min(fore, back, @)" : {"min":1e-3},

			"HyperMinX(fore, back, gamma)" : {"min":1e-3},
			"?HyperFindMin(fore, back, @)" : {"min":1e-3},
			
			"pat-no-starting-group" : {},
			"pat-no-ending-group" : {}
		},
		"format": "{pat}\t{pat-length}\t{p-value?}\n",
		"count": 100
	},

	"aliases" : {
		"inp" : {"desc":"input file",            "modify":["data.input"]},
		"ref" : {"desc":"reference file",        "modify":["data.reference"]},
		"p-min" : {"desc":"minimum for p-value", "modify":["output.filter.p-value.min"]},
		"p-max" : {"desc":"maximum for p-value", "modify":["output.filter.p-value.max"]}
	}
}
