{
    "Data": {
    	"fore" : { "File" : "" },
    	"back" : { "File" : "" }
    },
    "Alphabet": {
        "Separator": "",
        "Groups": {"." : {"elements":"ACDEFGHIKLMNPRQSTVWY"}}
    },
    "Extension": {
        "Method": "Groupex",
        "Filter": {
        	"PatGroups()" : {"max" : 3},
        	"PatLength()" : {"max" : 6},
        	"Matches(fore)" : {"min" : 20},
        	"NoStartingGroup" : {}	
        }
    },
    "Output": {
        "Order": "Hyper(fore, back)",
        "Sort": "desc",
        "Filter": {
        	"MatchesRatio(fore, back)" : {"min" : 2},
        	"HyperApprox(fore, back)" : {"max" : 1e-3},
        	"NoStartingGroup" : {},
        	"NoEndingGroup" : {}
        },
        "Queue": ""
    },
    "Print": {
        "Count": 10,
        "ShowHeader": true,
        "Header": "",
        "Format": "Pat?()\tPatLength()\tPatGroups()\tPatChars()\tMatches(fore)\tMatches(back)\tMatchesRatio(fore,back)\tHyper(fore,back)\n"
    },
    "Aliases": {
    	"inp" : {"desc":"input file",            "modify":["data.fore.file"]},
		"ref" : {"desc":"reference file",        "modify":["data.back.file"]},
		"p-min" : {"desc":"minimum for p-value", "modify":["output.filter.p-value.min"]},
		"p-max" : {"desc":"maximum for p-value", "modify":["output.filter.p-value.max"]}
    }
}