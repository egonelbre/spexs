{
	"Data": {
		"fore" : { "File" : "$inp$" },
		"back" : { "File" : "$ref$" }
	},
	"Alphabet": {
		"Separator": "",
		"Groups": {"." : {"elements":"ACDEFGHIKLMNPRQSTVWY"}}
	},
	"Extension": {
		"Method": "Group",
		"Filter": {
			"PatGroups()" : {"max" : 3},
			"PatLength()" : {"max" : 6},
			"Matches(fore)" : {"min" : 20},
			"NoStartingGroup" : {}	
		}
	},
	"Output": {
		"Order": "Hyper(fore, back)",
		"Sort": "desc",
		"Filter": {
			"MatchesRatio(fore, back)" : {"min" : 2},
			"HyperApprox(fore, back)" : {"max" : 1e-3},
			"NoStartingGroup" : {},
			"NoEndingGroup" : {}
		},
		"Queue": ""
	},
	"Print": {
		"Count": 100,
		"ShowHeader": true,
		"Header": "",
		"Format": "Pat?()\tPatLength()\tPatGroups()\tPatChars()\tMatches(fore)\tMatches(back)\tMatchesRatio(fore,back)\tHyper(fore,back)\n"
	}
}