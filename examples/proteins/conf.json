{
	"Dataset": {
		"fore" : { "File" : "$inp$" },
		"back" : { "File" : "$ref$" }
	},
	"Reader" : {
		"Method" : "Counted",
		"Separator" : "",
		"CountSeparator": "\t"
	},
	"Extension": {
		"Method": "Group",
		"Groups" : {
			"." : { "elements" : "ACDEFGHIKLMNPRQSTVWY"}
		},
		"Extendable": {
			"PatGroups()" : {"max" : 3},
			"PatLength()" : {"max" : 6},
			"Matches(fore)" : {"min" : 20},
			"NoStartingGroup" : {"enabled": true}
		},
		"Outputtable" : {
			"MatchesRatio(fore, back)" : {"min" : 2},
			"HyperApprox(fore, back)" : {"max" : 1e-3},
			"NoStartingGroup" : {},
			"NoEndingGroup" : {}
		}
	},
	"Output": {
		"Method" : "Sorted",
		"SortBy": ["-Hyper(fore, back)"],
		"Count": 100
	},
	"Printer" : {
		"Method" : "Formatted",
		"ShowHeader": true,
		"Reverse": true,
		"Header": "",
		"Format": "Pat?()\tPatLength()\tPatGroups()\tPatChars()\tMatches(fore)\tMatches(back)\tMatchesRatio(fore,back)\tHyper(fore,back)\n"
	}
}