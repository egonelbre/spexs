{
	"data" : {
		"input" : "",
		"reference" : ""
	},

	"alphabet" : {
		"separator" : "",
		"groups" : {
			".": {"elements":"ACDEFGHIKLMNPRQSTVWY"}
		}
	},

	"extension" : {
		"method" : "group",
		"filter" : {
			"pat-groups" : {"max":4},
			"pat-length" : {"max":8},
			"query-match-seqs" : {"min":10}
		}
	},
	"output" : {
		"order" : "match-hyper-up-pvalue",
		"sort" : "desc",
		"filter" : {
			"match-ratio" : {"min":2},
			"match-hyper-up-pvalue-approx" : {"max":1e-2}
		},
		"format": "{pat}\t{pat-length}\t{pat-groups}\t{pat-chars}\t{query-match-seqs}\t{back-match-seqs}\t{match-ratio}\t{match-hyper-up-pvalue}\n",
		"count": 100
	},
	"aliases" : {
		"inp" : {"desc":"input file",            "modify":["data.input"]},
		"ref" : {"desc":"reference file",        "modify":["data.reference"]},
		"p-min" : {"desc":"minimum for p-value", "modify":["output.filter.p-value.min"]},
		"p-max" : {"desc":"maximum for p-value", "modify":["output.filter.p-value.max"]}
	}
}
