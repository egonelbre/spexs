{
	"Data": {
		"inp" : { "File" : "" }
	},
	"Alphabet": {
		"Separator": " "
	},
	"Extension": {
		"Method": "Star",
		"Filter": {
			"PatGroups()" : {"max" : 3},
			"PatLength()" : {"max" : 6},
			"Matches(inp)" : {"min" : 10},
			"NoStartingGroup" : {}	
		}
	},
	"Output": {
		"Order": "HyperOptimal(inp)",
		"Sort": "desc",
		"Filter": {
			"Matches(inp)" : {"min" : 10},
			"HyperOptimal(inp)" : {"max" : 1e-3},
			"NoStartingGroup" : {},
			"NoEndingGroup" : {}
		},
		"Queue": ""
	},
	"Print": {
		"Count": 100,
		"ShowHeader": true,
		"Header": "<$fg/#fg>\t<$all/#all>\tp-value\tpattern\n",
		"Format": "HyperOptimal?(inp)\t<Matches(inp)/Seqs(inp)>\tHyperOptimal(inp)\tPat?()\n"
	},
	"Aliases": {
		"inp" : {"desc":"input file", "modify":["Data.inp.File"]}
	}
}