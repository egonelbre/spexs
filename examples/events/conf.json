{
	"Dataset": {
		"fore" : { "File" : "$inp$" },
		"back" : { "File" : "$ref$" }
	},
	"Reader" : {
		"Method" : "Delimited",
		"Separator" : "",
		"CountSeparator": ""
	},
	"Extension": {
		"Method": "StarExact",
		"Groups" : {
			"." : { "elements" : "ABCDFGHIXYZ"}
		},
		"Extendable": {
			"PatLength()" : {"max" : 7},
			"PatGroups()" : {"max" : 3},
			"Matches(fore)" : {"min" : 300},
			"NoStartingGroup()" : {},
			"NoTokens()" : {"Tokens":"E"},
		},
		"Outputtable" : {
			"PatLength()" : {"min" : 3},
			"Hyper(fore, back)" : {"max" : 1e-10},
			"NoEndingGroup()" : {}
		}
	},
	"Output": {
		"SortBy": ["MatchesPropRatio(fore,back)", "Matches(fore)"],
		"Count": 100
	},
	"Printer" : {
		"Method" : "Formatted",
		"ShowHeader": true,
		"Reverse": true,
		"Header": "",
		"Format": "Pat?()\t\tMatches(fore)/Total(fore)\tMatches(back)/Total(back)\tMatchesPropRatio(fore,back)\n"
	}
}