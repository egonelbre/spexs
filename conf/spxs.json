{
	"data" : {
		"input" : "",
		"reference" : ""
	},

	"alphabet" : {
		"characters" : "ACDEFGHIKLMNPRQSTVWY",
		"groups" : {
			".": {"group":"ACDEFGHIKLMNPRQSTVWY"}
		}
	},

	"extension" : {
		"method" : "regexp",
		"args" : {"star":{ "max-gap": 3}},
		"filter" : {
			"query-match-seqs-prop" : {"min":0.8},
			"no-starting-group" : {}
		}
	},
	"output" : {
		"order" : "match-hyper-up-pvalue",
		"sort" : "desc",
		"args" : {"p-value":{}},
		"filter" : {
			"query-match-seqs-prop" : {"min":0.8},
			"no-ending-group" : {},
			"no-starting-group" : {}
		},
		"count": 100,
		"format": "{pat}\t{pat-chars}\t{match-hyper-up-pvalue}\n"
	},
	"aliases" : {
		"inp" : {"desc":"input file",            "modify":["data.input"]},
		"ref" : {"desc":"reference file",        "modify":["data.reference"]},
		"p-min" : {"desc":"minimum for p-value", "modify":["output.filter.p-value.min"]},
		"p-max" : {"desc":"maximum for p-value", "modify":["output.filter.p-value.max"]}
	}
}
